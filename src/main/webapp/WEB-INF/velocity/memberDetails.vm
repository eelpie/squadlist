#parse('includes/head.vm')

<h3>$member.firstName $member.lastName</h3>

#parse('includes/nav.vm')

<div class="row">
	<div class="span3 offset1">

	<p>$member.firstName $member.lastName</p>
	<p>$!member.gender</p>
	<p>$!member.dateOfBirth</p>

	<p>Email address: $!member.emailAddress</p>
	<p>Contact number: $!member.contactNumber</p>

	<p>Weight: $!member.weight</p>
	<p>RegistrationNumber: $!member.registrationNumber</p>

	Squads:
	<ul>
	#foreach($squad in $member.squads)
	 	<li>$squad.name</li>
	#end
	</ul>
	
	<a href="$urlBuilder.editMemberUrl($member)"><button class="btn btn-primary" type="button">Edit details</button></a>
	
	</div>
</div>



<hr>
