#parse('includes/head.vm')
#parse('includes/nav.vm')

<div class="row">
		
	<div class="span2 bs-docs-sidebar">
		#parse('includes/squadnav.vm')
	</div>
	
	<div class="span9">
		<h3>$squad.name availability</h3>

		<table>
			<tr>
			<td></td>
			#foreach($outing in $outings) 
			<td>#parse('includes/outing.vm')</td>
			#end
		</tr>

		#foreach( $member in $members )
			<tr>
				<td>#parse('includes/member.vm')</td>
				#foreach($outing in $outings) 
					#set($key = "${outing.id}-$member.id")
					<td>
						<span class="label label-info">
							$!availability.get($key)
						</span>		
					</td>
				#end
			</tr>
		#end
		</tr>
		</table>
		<hr>

	</div>
</div>
</div>
